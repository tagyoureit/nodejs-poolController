{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": ["./src"],
        "includePattern": ".+\\.ts(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "destination": "./docs/",
    "opts": {
        "recurse": true,
        "destination": "./docs/"
    }
}